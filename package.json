{
  "name": "@p-stack/turborepo-template",
"description": "A modern, lightweight Turborepo template with essential open-source configurations for p-stack organization projects",
  "keywords": [
    "turborepo",
    "template",
    "monorepo",
    "typescript",
    "open-source",
    "github-template",
    "p-stack",
    "modern-tooling",
    "conventional-commits",
    "repository-template"
  ], "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=22"
  }
}
